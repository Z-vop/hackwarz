{
  "name": "hackwarz",
  "version": "1.0.0",
  "description": "A hacking game.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm run test -- --watch",
    "start": "nodemon index.js --exec babel-node --presets env",
    "test:concepts": "mocha concepts --compilers js:babel-register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Z-vop/hackwarz.git"
  },
  "author": "Oliver O'Rourke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Z-vop/hackwarz/issues"
  },
  "homepage": "https://github.com/Z-vop/hackwarz#readme",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "express": "^4.15.3",
    "immutable": "^3.8.1",
    "nedb": "^1.8.0",
    "redux": "^3.6.0",
    "socket.io": "^1.7.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "should": "^11.2.1"
  }
}
